{
  "expo": {
    "name": "Bounce Design System",
    "slug": "bounce-storybook",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./storybook/native/assets/icon.png",
    "splash": {
      "image": "./storybook/native/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "entryPoint": "./storybook/native/App"
  }
}
